{
    "name": "karolornoch/oneloginsaml-bundle",
    "type": "symfony-bundle",
    "description": "OneLogin SAML Bundle for Symfony2",
    "keywords": [
        "onelogin",
        "saml",
        "sso"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "hslavich",
            "email": "hernan.slavich@gmail.com"
        }
    ],
    "require": {
        "karolornoch/php-saml": "3.1.1",
        "symfony/framework-bundle": "~2.3 || ~3.0 || ^4.0",
        "symfony/security-bundle": "~2.3 || ~3.0 || ^4.0"
    },
    "require-dev": {
        "doctrine/orm": "~2.4",
        "phpunit/phpunit": "~5.7",
        "satooshi/php-coveralls": "~1.0",
        "symfony/event-dispatcher": "~2.3 || ~3.0 || ^4.0",
        "symfony/phpunit-bridge": "~2.7 || ~3.0 || ^4.0"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Hslavich\\OneloginSamlBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    }
}
