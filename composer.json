{
    "name": "hslavich/oneloginsaml-bundle",
    "type": "symfony-bundle",
    "description": "OneLogin SAML Bundle for Symfony",
    "keywords": ["onelogin", "saml", "sso"],
    "license": "MIT",
    "authors": [
        {
            "name": "hslavich",
            "email": "hernan.slavich@gmail.com"
        }
    ],
    "require": {
        "symfony/framework-bundle": "^5.1.5",
        "symfony/security-bundle": "^5.1",
        "onelogin/php-saml": "^3.0",
        "symfony/deprecation-contracts": "^2.1"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^5.1",
        "doctrine/orm": "~2.3",
        "php-coveralls/php-coveralls": "^2.2",
        "phpunit/phpunit": "^9.0",
        "dms/phpunit-arraysubset-asserts": "^0.2.0"
    },
    "conflict": {
        "symfony/security-bundle": ">=5.2"
    },
    "autoload": {
        "psr-4": { "Hslavich\\OneloginSamlBundle\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    }
}
