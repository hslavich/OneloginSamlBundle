{
    "name": "hslavich/oneloginsaml-bundle",
    "type": "symfony-bundle",
    "description": "OneLogin SAML Bundle for Symfony2",
    "keywords": ["onelogin", "saml", "sso"],
    "license": "MIT",
    "authors": [
        {
            "name": "hslavich",
            "email": "hernan.slavich@gmail.com"
        }
    ],
    "require": {
        "symfony/framework-bundle": "~2.3|~3.0|^4.0",
        "symfony/security-bundle": "~2.3|~3.0|^4.0",
        "onelogin/php-saml": "^3.0"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "~2.7|~3.0|^4.0",
        "doctrine/orm": "~2.3",
        "satooshi/php-coveralls": "~1.0",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "psr-4": { "Hslavich\\OneloginSamlBundle\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    }
}
